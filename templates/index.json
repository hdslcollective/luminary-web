            "max_width": "normal",
            "alignment": "left",
            "type_preset": "custom",
            "font": "var(--font-heading--family)",
            "font_size": "",
            "line_height": "tight",
            "letter_spacing": "normal",
            "case": "none",
            "wrap": "pretty",
            "color": "",
